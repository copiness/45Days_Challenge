class Solution {
    public List<Integer> findDuplicates(int[] nums) {
        Map<Integer,Integer> erp = new HashMap<>();
        List<Integer> ans = new ArrayList<>();

         for (int num : nums) {
            erp.put(num, erp.getOrDefault(num, 0) + 1);
        }

        for (Map.Entry<Integer, Integer> entry : erp.entrySet()) {
            if (entry.getValue() > 1) {
                ans.add(entry.getKey());
            }
        }

        Collections.sort(ans);
        return ans;
    }
}
